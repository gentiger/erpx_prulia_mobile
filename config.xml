<?xml version='1.0' encoding='utf-8'?>
<widget id="com.erpx.client.prulia" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PRULIA</name>
    <description>
		Prudential Life Insurance Agency Association.
	</description>
    <author email="support@erpx.com.my" href="http://www.erpx.com.my">
		Alpha Herald Management
	</author>
    <content src="login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://167.88.124.17/" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="res/android/icons/ldpi.png" />
        <icon density="mdpi" src="res/android/icons/mdpi.png" />
        <icon density="hdpi" src="res/android/icons/hdpi.png" />
        <icon density="xhdpi" src="res/android/icons/xhdpi.png" />
        <icon density="xxhdpi" src="res/android/icons/xxhdpi.png" />
        <icon density="xxxhdpi" src="res/android/icons/xxxhdpi.png" />
        <splash density="land-hdpi" src="res/android/splash/land-hdpi.png" />
        <splash density="land-ldpi" src="res/android/splash/land-ldpi.png" />
        <splash density="land-mdpi" src="res/android/splash/land-mdpi.png" />
        <splash density="land-xhdpi" src="res/android/splash/land-xhdpi.png" />
        <splash density="land-xxhdpi" src="res/android/splash/land-xxhdpi.png" />
        <splash density="land-xxxhdpi" src="res/android/splash/land-xxxhdpi.png" />
        <splash density="port-hdpi" src="res/android/splash/port-hdpi.png" />
        <splash density="port-ldpi" src="res/android/splash/port-ldpi.png" />
        <splash density="port-mdpi" src="res/android/splash/port-mdpi.png" />
        <splash density="port-xhdpi" src="res/android/splash/port-xhdpi.png" />
        <splash density="port-xxhdpi" src="res/android/splash/port-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="res/android/splash/port-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#ffffff" />
        <preference name="StatusBarStyle" value="default" />
        <icon height="57" src="res/ios/icons/appicon.png" width="57" />
        <icon height="114" src="res/ios/icons/appicon@2x.png" width="114" />
        <icon height="60" src="res/ios/icons/appicon-60.png" width="60" />
        <icon height="120" src="res/ios/icons/appicon-60@2x.png" width="120" />
        <icon height="180" src="res/ios/icons/appicon-60@3x.png" width="180" />
        <icon height="72" src="res/ios/icons/appicon-72.png" width="72" />
        <icon height="144" src="res/ios/icons/appicon-72@2x.png" width="144" />
        <icon height="76" src="res/ios/icons/appicon-76.png" width="76" />
        <icon height="152" src="res/ios/icons/appicon-76@2x.png" width="152" />
        <icon height="29" src="res/ios/icons/appicon-Small.png" width="29" />
        <icon height="58" src="res/ios/icons/appicon-Small@2x.png" width="58" />
        <icon height="87" src="res/ios/icons/appicon-Small@3x.png" width="87" />
        <icon height="40" src="res/ios/icons/appicon-Small-40.png" width="40" />
        <icon height="80" src="res/ios/icons/appicon-Small-40@2x.png" width="80" />
        <icon height="50" src="res/ios/icons/appicon-Small-50.png" width="50" />
        <icon height="100" src="res/ios/icons/appicon-Small-50@2x.png" width="100" />
        <splash height="480" src="res/ios/splash/Default.png" width="320" />
        <splash height="960" src="res/ios/splash/Default@2x.png" width="640" />
        <splash height="1024" src="res/ios/splash/Default-Portrait.png" width="768" />
        <splash height="2048" src="res/ios/splash/Default-Portrait@2x.png" width="1536" />
        <splash height="768" src="res/ios/splash/Default-Landscape.png" width="1024" />
        <splash height="1536" src="res/ios/splash/Default-Landscape@2x.png" width="2048" />
        <splash height="1136" src="res/ios/splash/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/ios/splash/Default-667h@2x.png" width="750" />
        <splash height="2208" src="res/ios/splash/Default-736h.png" width="1242" />
        <splash height="1242" src="res/ios/splash/Default-Landscape-736h.png" width="2208" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.0" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
